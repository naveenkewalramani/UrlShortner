<%= form_for @short_domain, url:short_domain_create_path   do |form| %>
  <p>Domain Name: <%= form.text_field :domain%> </br></p>
  <p>Short Domain : <%= form.text_field :prefix%> <br /></p>
  <p>Find : <%=  form.submit 'create' %><br/>
<% end %>
<div class="errors">
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value %></div>
  <% end %>
</div>

<button>BACK : <%= link_to 'back', new_url_path  %> </button>
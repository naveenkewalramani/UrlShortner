<p>WELCOME TO LOGIN PAGE</p>
<div class="Login_Form">
  <h1>Login</h1>
  <%= form_for @user, :url => users_login_path do |t| %>
    <p> Username:</br> <%= t.text_field :username%> </p>
    <p> Email:</br> <%= t.text_field :email%> </p>
    <p> Password:</br> <%= t.password_field :password%></p>
    <%= t.submit :Login %>
  <% end %>
</div>
